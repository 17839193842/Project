    <script>
        $(function () {            
            //类别标签点击切换
            var $type=$(".search-result-type div div a"),
                $ul=$('.search-result ul'),
                index;
            //全部类别
            var $all=$ul.find('li').clone();
            $ul.eq(0).append($all);
            var changeType=function(index){
                $type.removeClass('active');
                $type.eq(index).addClass('active');
                $ul.css('display','none');
                $ul.eq(index).fadeIn();
                $('.video-count').html('共 '+$ul.eq(index).find('li').length+' 个视频教程');
            };
            $type.on('click',function(){
                index=$type.index(this);
                changeType(index);
            });
            //判断类别
            var $href=window.location.href,
                hrefStart=$href.indexOf('#');
            hrefStart!=-1 ? changeType($href.substring(hrefStart+1,$href.length)-1) : changeType(0);
            
        });
    </script>
